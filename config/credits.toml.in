vo_id = your_vo_id_here
perun_rpc_base_url = "https://perun.elixir-czech.cz/krb/rpc/json"

[service_user]
login = your_login_here
password = your_password_here

[logging]
version=1
[logging.formatters.simple]
format = "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
[logging.formatters.access]
format = '%a %t "%r" %s %b "%{Referer}i" "%{User-Agent}i"'

[logging.handlers.console]
class = 'logging.StreamHandler'
level = 'DEBUG'
stream = 'ext://sys.stdout'

[logging.loggers."os_credits.credits"]
level = 'INFO'
handlers = ['console']
formatter = 'simple'

[logging.loggers."os_credits.main"]
level = 'INFO'
handlers = ['console']
formatter = 'simple'

[logging.loggers."os_credits.perun.groupsManager"]
level = 'DEBUG'
handlers = ['console']
formatter = 'simple'

[logging.loggers."aiohttp.server"]
level = 'DEBUG'
handlers = ['console']
formatter = 'simple'

[logging.loggers."aiohttp.access"]
level = 'DEBUG'
handlers = ['console']
formatter = 'access'
